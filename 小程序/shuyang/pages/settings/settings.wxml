<view class="container">
  <view class="wx-cells">
    <view class="wx-cell">
      <image class="wx-cell-icon user-avatar" src="{{userInfoAvatar}}"></image>
      <view class="user-info">
        <text class="user-info-text">{{nickName}}</text>
        <text class="user-info-text user-info-uid">{{sex}}</text>
      </view>
    </view>
  </view>
  <view class="wx-cells">
    <view class="wx-cell">
      <image class="wx-cell-icon" src="../../images/feedback.png"></image>
    <contact-button class="contact" type="default-light" size="12" session-from="weapp" style="opacity:0;position:absolute;top:296rpx;left:90rpx;">  
    </contact-button>  
    <contact-button class="contact" type="default-light" size="12" session-from="weapp" style="opacity:0;position:absolute;top:296rpx;left:130rpx; ">  
    </contact-button>  
    <contact-button class="contact" type="default-light" size="12" session-from="weapp" style="opacity:0;position:absolute;top:296rpx;left:170rpx; ">  
    </contact-button>  
    <contact-button class="contact" type="default-light" size="12" session-from="weapp" style="opacity:0;position:absolute;top:296rpx;left:230rpx; ">  
    </contact-button>  
    <text class="wx-cell-text">在线咨询</text>  
    </view>
  </view>
  <view class="wx-cells">
    <view class="wx-cell" data-route="../about/about" bindtap="navToPage">
      <image class="wx-cell-icon" src="../../images/about.png"></image>
      <text class="wx-cell-text">关于我们</text>
      <image class="wx-cell-arrow" src="../../images/right-gray.png"></image>
    </view>
  </view>
</view>